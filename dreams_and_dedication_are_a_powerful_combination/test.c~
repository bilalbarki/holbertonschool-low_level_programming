#include "my_functions.h"

void print_number(int n)
{
  int count,i,temp,j;
  temp=0;
  i=n;
  for (count=0;i!=0;count++)
    i=i/10;
  if (n<0)
    print_char('-');
  
  int x;
  do{
    i=n;
    x=count;
    for(;x>1;x--){
      i=i/10;
    }
    j=i-(temp*10);
    temp=i;
    if (j<0){
      j=j*(-1);
    }
    print_char(j+48);
    count--;
  }
  while(count>0);
}
